{
  "name": "wwwaxe",
  "version": "0.2.0",
  "description": "Strip non-content data from HTML for agentic consumption. Retains meaningful page structure while dramatically reducing token count.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "html",
    "strip",
    "condense",
    "tokens",
    "llm",
    "agentic",
    "web",
    "scraping",
    "preprocessing"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simen/wwwaxe.git"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "htmlparser2": "^9.1.0",
    "dom-serializer": "^2.0.0",
    "domhandler": "^5.0.3",
    "domutils": "^3.1.0"
  }
}
